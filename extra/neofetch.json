{
    "name": "neofetch",
    "description": "A command-line system information tool written in bash 3.2+",
    "version": "7.1.0",
    "maintainer": "dylanaraps",
    "installation": [ 
		"curl -L -o neofetch.tar.gz https://github.com/dylanaraps/neofetch/archive/refs/tags/7.1.0.tar.gz",
		"sudo tar -xvf neofetch.tar.gz", 
		"mkdir -p /usr/share/man/man1", 
		"cp -p neofetch/neofetch /hl-bin/neofetch" ,
		"cp -p neofetch/neofetch.1 /usr/share/man/man1/" ,
		"rm -r neofetch/",
		"rm neofetch.tar.gz"
	],
	"uninstallation": [
		"echo sdf",
		"hey"
	]
}
